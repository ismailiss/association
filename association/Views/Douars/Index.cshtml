@model IEnumerable<association.Models.Douar>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomFr)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NomAr)
            </th>
       
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomFr)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NomAr)
            </td>

            <td>
                <a asp-action="Edit" asp-route-id="@item.DouarID">Modifier</a> |
                <a asp-action="Details" asp-route-id="@item.DouarID">Détails</a> |
                <a asp-action="Delete" asp-route-id="@item.DouarID">Supprimer</a>
            </td>
        </tr>
}
    </tbody>
</table>
